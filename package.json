{
  "name": "build-config",
  "version": "0.3.0",
  "description": "JS Build Recipes",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn rollup-all && yarn wp-all",
    "rollup": "rollup -c --watch",
    "rollup-all": "npm-run-all --parallel rollup-preact rollup-react rollup-riot rollup-svelte rollup-vue rollup-vanilla rollup-marko",
    "rollup-marko": "rollup -c ./rollup/marko.js",
    "rollup-preact": "rollup -c ./rollup/preact.js",
    "rollup-react": "rollup -c ./rollup/react.js",
    "rollup-riot": "rollup -c ./rollup/riot.js",
    "rollup-svelte": "rollup -c ./rollup/svelte.js",
    "rollup-vanilla": "rollup -c ./rollup/vanilla.js",
    "rollup-vue": "rollup -c ./rollup/vue.js",
    "compare": "yarn rollup-build-all",
    "start": "forever -m 5 --minUptime 1000 --spinSleepTime 500 server.js",
    "run-rollup": "npm-run-all --parallel rollup watch",
    "run-wp": "npm-run-all --parallel wp watch",
    "watch": "nodemon server.js",
    "wp": "webpack",
    "wp-all": "npm-run-all --parallel wp-marko wp-preact wp-react wp-riot wp-react wp-svelte wp-vanilla wp-vue",
    "wp-marko": "webpack --config ./webpack/marko",
    "wp-preact": "webpack --config ./webpack/preact",
    "wp-riot": "webpack --config ./webpack/riot",
    "wp-react": "webpack --config ./webpack/react",
    "wp-svelte": "webpack --config ./webpack/svelte",
    "wp-vanilla": "webpack --config ./webpack/vanilla",
    "wp-vue": "webpack --config ./webpack/vue"
  },
  "engines": {
    "node": "^7.0.0",
    "npm": "^4.0.0"
  },
  "dependencies": {
    "@huck/browser": "^0.1.1",
    "@huck/core": "^0.1.5",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-babili": "^0.0.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babili": "^0.0.10",
    "body-parser": "^1.16.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "express": "^4.14.1",
    "forever": "^0.15.3",
    "ip": "^1.1.4",
    "livereload": "^0.6.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "marko": "^4.2.0",
    "marko-loader": "^1.3.0",
    "markoify": "^2.2.2",
    "multer": "^1.3.0",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "pm2": "^2.3.0",
    "postcss": "^5.2.11",
    "postcss-cssnext": "^2.9.0",
    "preact": "^7.2.0",
    "pug": "^2.0.0-beta10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "redis": "^2.6.5",
    "redux": "^3.6.0",
    "riot": "^3.3.1",
    "riot-tag-loader": "^1.0.0",
    "rollup": "^0.41.4",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-browserify-transform": "^0.1.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-minify": "^1.0.3",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-pug": "^0.1.2",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-riot": "^1.1.0",
    "rollup-plugin-sass": "^0.4.8",
    "rollup-plugin-scss": "^0.2.0",
    "rollup-plugin-stylus-css-modules": "^1.2.3",
    "rollup-plugin-svelte": "^1.6.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-plugin-visualizer": "^0.2.0",
    "rollup-plugin-vue": "^2.2.21",
    "rollup-watch": "^3.2.2",
    "router5": "^4.4.1",
    "serve-favicon": "^2.4.1",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "svelte": "^1.14.0",
    "svelte-loader": "^2.0.1",
    "uglify-js": "^2.8.7",
    "uglifyjs-webpack-plugin": "^0.4.1",
    "vue": "^2.2.1",
    "vue-server-renderer": "^2.2.1",
    "webpack": "^2.3.3",
    "winston": "^2.3.1"
  }
}
